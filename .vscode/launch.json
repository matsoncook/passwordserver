{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI: Uvicorn (app.py)",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8042",
                "--reload"
            ],
            "jinja": true,
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}
